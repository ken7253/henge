{
  "manifest_version": 3,
  "version": "0.0.1",
  "short_name": "HENGE",
  "name": "HENGE custom stylesheet Manager",
  "description": "Speedy loading of custom CSS",
  "background": {
    "service_worker": "js/background.js"
  },
  "action": {
    "default_popup": "index.html"
  },
  "options_ui": {
    "page": "index.html"
  },
  "permissions": [
    "scripting",
    "tabs"
  ],
  "host_permissions": [
    "https://*/*"
  ]
}